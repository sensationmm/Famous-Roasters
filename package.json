{
  "name": "frontend",
  "version": "1.102.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@sentry/react": "^7.18.0",
    "@sentry/tracing": "^7.18.0",
    "@types/react-instantsearch-dom": "^6.12.3",
    "algoliasearch": "^4.14.2",
    "date-fns": "^2.29.1",
    "graphql": "16.5.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-instantsearch-hooks-web": "^6.38.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --maxWorkers=50%",
    "test:watchAll": "react-scripts test --coverage --watchAll",
    "test:watch": "react-scripts test --coverage --runInBand",
    "test:ci": "react-scripts test --coverage --watchAll=false",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prettier": "prettier --check src/**/*.{ts,tsx}",
    "prettier:write": "prettier --check src/**/*.{ts,tsx} --write",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-aws-s3 --out=.storybook-out",
    "git-prune": "git branch --merged main | grep -v '^[ *]*main$' | xargs git branch -d"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,d.ts}": [
      "npm run prettier:write",
      "npm run lint:fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-amplify/ui-react": "^3.0.3",
    "@babel/core": "^7.17.8",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@headlessui/react": "^0.0.0-insiders.dd2feef",
    "@heroicons/react": "^1.0.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@shopify/hydrogen": "^1.6.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.19",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/testing-library": "^0.0.11",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.2",
    "aws-amplify": "^4.3.31",
    "aws-amplify-react": "^5.1.9",
    "babel-loader": "^8.2.3",
    "eslint": "^8.19.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "graphql.macro": "^1.4.2",
    "husky": "^7.0.4",
    "i18next": "^21.8.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "rc-field-form": "^1.27.0",
    "react-hook-form": "^7.29.0",
    "react-i18next": "^11.16.9",
    "semantic-release": "^19.0.3",
    "storybook-react-i18next": "^1.0.18",
    "swiper": "^8.1.4",
    "tailwindcss": "^3.0.23",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.72.0"
  },
  "resolutions": {
    "async": "^2.6.4",
    "ejs": "^3.1.7",
    "glob-parent": "^5.1.2",
    "nth-check": "^2.0.1",
    "parse-path": "^5.0.0",
    "trim": "^0.0.3",
    "trim-newlines": "^3.0.1",
    "webpack": "^5.72.0",
    "workbox-webpack-plugin": "^6.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "swiper/css$": "identity-obj-proxy",
      "swiper/css/a11y$": "identity-obj-proxy",
      "swiper/css/keyboard$": "identity-obj-proxy",
      "swiper/css/navigation$": "identity-obj-proxy",
      "swiper/css/pagination$": "identity-obj-proxy",
      "swiper/react": "<rootDir>/node_modules/swiper/react/swiper-react.js"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(@shopify/hydrogen)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.tsx",
      "<rootDir>/.*.stories.tsx$"
    ]
  }
}
